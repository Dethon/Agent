assistants.herfluffnes.com {
    tls {
        dns cloudflare {env.CF_API_TOKEN}
    }

    handle /hubs/* {
        reverse_proxy agent:8080
    }

    handle {
        reverse_proxy webui:8080
    }
}

localhost {
    tls internal

    handle /hubs/* {
        reverse_proxy agent:8080
    }

    handle {
        reverse_proxy webui:8080
    }
}
